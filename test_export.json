    {
      "_id": "546e93c4dd8a2814",
      "name": "Brutal Critical (1 die)",
      "type": "feat",
      "img": "icons/svg/book.svg",
      "system": {
        "description": {
          "value": "<p>Beginning at 9th level, you can roll one additional weapon damage die when determining the extra damage for a critical hit with a melee attack. This increases to two additional dice at 13th level and three additional dice at 17th level.</p>"
        },
        "type": {
          "value": "class"
        },
        "source": {
