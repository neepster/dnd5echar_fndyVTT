    {
      "_id": "cf91b3cae49c2d08",
      "name": "Disciple of Life",
      "type": "feat",
      "img": "icons/svg/book.svg",
      "system": {
        "description": {
          "value": "<p>Also starting at 1st level, your healing spells are more effective. Whenever you use a spell of 1st level or higher to restore hit points to a creature, the creature regains additional hit points equal to 2 + the spell's level.</p>"
        },
        "type": {
          "value": "class"
        },
        "source": {
